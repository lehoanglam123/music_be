<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias alias="Artist" type="com.spring.security.model.Artist" />
		<typeAlias alias="Song" type="com.spring.security.model.Song" />
		<typeAlias alias="Genre" type="com.spring.security.model.Genre" />
		<typeAlias alias="Album" type="com.spring.security.model.Album" />
		<typeAlias alias="Account" type="com.spring.security.model.Account" />
		<typeAlias alias="Token" type="com.spring.security.model.Token" />
		<typeAlias alias="SongEntity" type="com.spring.security.entity.SongEntity" />
		<typeAlias alias="SongSearchEntity" type="com.spring.security.entity.SongSearchEntity" />
		<typeAlias alias="ArtistEntity" type="com.spring.security.entity.ArtistEntity" />
		<typeAlias alias="AlbumEntity" type="com.spring.security.entity.AlbumEntity" />
		<typeAlias alias="ListSongId" type="com.spring.security.request.ListSongId" />
		<typeAlias alias="ListGenreId" type="com.spring.security.request.ListGenreId" />
		<typeAlias alias="ListArtistId" type="com.spring.security.request.ListArtistId" />
		<typeAlias alias="ListAlbumId" type="com.spring.security.request.ListAlbumId" />
	</typeAliases>

	<mappers>
		<mapper resource="Mapper/ArtistRepository.xml" />
		<mapper resource="Mapper/SongRepository.xml" />
		<mapper resource="Mapper/GenreRepository.xml" />
		<mapper resource="Mapper/AlbumRepository.xml" />
		<mapper resource="Mapper/AccountRepository.xml" />
		<mapper resource="Mapper/TokenRepository.xml" />
	</mappers>

</configuration>